<h1>Audit logs</h1>
<table class="table table-hover table-bordered">
	<thead>
		<tr>
			<th>
				#
			</th>
			<th>
				Employee
			</th>
			<th>
				Week Starting
			</th>
			<th>
				Confirmed At
			</th>
			<th>
                Status
			</th>
		</tr>
	</thead>
    <tbody id="#audit_logs">
        <%= render @audit_logs %>
    </tbody>
</table>
<div id="paginator" class="text-center">
	<%= paginate @audit_logs %>
</div>
